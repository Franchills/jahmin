{
  "name": "jahmin",
  "productName": "Jahmin",
  "version": "1.0.0",
  "description": "Jahmin",
  "main": "dist/main/main.js",
  "scripts": {
    "tsc-watch:main": "cd app/main && npx tsc --watch",
    "tsc-watch:preload": "cd app/preload && npx tsc --watch",
    "tsc-watch:workers": "cd app/workers && npx tsc --watch",
    "electron": "electron .",
    "svelte:dev": "cd app/svelte && npm run dev",
    "dev": "concurrently \"npm run tsc-watch:main\" \"npm run tsc-watch:preload\"  \"npm run tsc-watch:workers\" \"npm run svelte:dev\"",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "Francisco Gimeno",
    "email": "franchills@tuta.io"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_build"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "linux"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "options": {
              "maintainer": {
                "name": "Francisco Gimeno"
              }
            }
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "deepmerge": "^4.2.2",
    "dexie": "^3.2.2",
    "dompurify": "^2.3.12",
    "electron-squirrel-startup": "^1.0.0",
    "exiftool-vendored": "^17.0.1",
    "freezeframe": "^5.0.2",
    "fuse.js": "^6.6.2",
    "izitoast": "^1.4.0",
    "music-metadata": "^7.12.6",
    "node-id3": "^0.2.3",
    "sanitize-filename": "^1.6.3",
    "sharp": "^0.30.7",
    "sirv-cli": "^2.0.0",
    "sortablejs": "^1.15.0",
    "spark-md5": "^3.0.2",
    "string-hash": "^1.1.3",
    "tippy.js": "^6.3.7",
    "xxhashjs": "^0.2.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.65",
    "@electron-forge/maker-deb": "^6.0.0-beta.65",
    "@electron-forge/maker-dmg": "6.0.0-beta.65",
    "@electron-forge/maker-rpm": "^6.0.0-beta.65",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.65",
    "@electron-forge/maker-zip": "^6.0.0-beta.65",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^2.0.1",
    "@types/node": "^18.7.6",
    "@types/sharp": "^0.30.5",
    "@types/spark-md5": "^3.0.2",
    "@types/xxhashjs": "^0.2.2",
    "chokidar": "^3.5.3",
    "concurrently": "^7.3.0",
    "electron": "20.0.2",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.7.4"
  }
}
