{
  "name": "jahmin",
  "productName": "Jahmin",
  "version": "1.0.0",
  "description": "Jahmin",
  "main": "dist/main/main.js",
  "scripts": {
    "tsc-watch:main": "cd app/main && npx tsc --watch",
    "tsc-watch:preload": "cd app/preload && npx tsc --watch",
    "tsc-watch:workers": "cd app/workers && npx tsc --watch",
    "electron": "electron .",
    "svelte:dev": "cd app/svelte && npm run dev",
    "dev": "concurrently \"npm run tsc-watch:main\" \"npm run tsc-watch:preload\"  \"npm run tsc-watch:workers\" \"npm run svelte:dev\"",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "Francisco Gimeno",
    "email": "franchills@tuta.io"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./dist/icons/logo"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_build",
            "setupIcon":"./dist/icons/logo.ico"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "linux"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "options": {
              "maintainer": {
                "name": "Francisco Gimeno"
              }
            }
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "deepmerge": "^4.2.2",
    "dexie": "^3.2.3",
    "dexie-observable": "^4.0.1-beta.13",
    "dompurify": "^2.4.3",
    "electron-squirrel-startup": "^1.0.0",
    "exiftool-vendored": "^19.0.0",
    "freezeframe": "^5.0.2",
    "fuse.js": "^6.6.2",
    "izitoast": "^1.4.0",
    "music-metadata": "^7.13.3",
    "node-id3": "^0.2.5",
    "sanitize-filename": "^1.6.3",
    "sharp": "^0.31.3",
    "sirv-cli": "^2.0.2",
    "sortablejs": "^1.15.0",
    "spark-md5": "^3.0.2",
    "string-hash": "^1.1.3",
    "tippy.js": "^6.3.7",
    "xxhashjs": "^0.2.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.4",
    "@electron-forge/maker-deb": "^6.0.4",
    "@electron-forge/maker-dmg": "6.0.4",
    "@electron-forge/maker-rpm": "^6.0.4",
    "@electron-forge/maker-squirrel": "^6.0.4",
    "@electron-forge/maker-zip": "^6.0.4",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "^11.0.0",
    "@tsconfig/svelte": "^3.0.0",
    "@types/node": "^18.11.18",
    "@types/sharp": "^0.31.1",
    "@types/spark-md5": "^3.0.2",
    "@types/xxhashjs": "^0.2.2",
    "autoprefixer": "^10.4.13",
    "chokidar": "^3.5.3",
    "concurrently": "^7.6.0",
    "electron": "^23.0.0",
    "postcss": "^8.4.21",
    "postcss-load-config": "^4.0.1",
    "rollup": "^2.77.4-1",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.3",
    "svelte-preprocess": "^5.0.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  }
}